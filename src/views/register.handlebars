<div>
    <h1>Register</h1>
    <form id="registerForm">
        <label>Name</label>
        <input name="name" id="nameRegister">
        <br>

        <label>Last Name</label>
        <input name="lastName" id="lastNameRegister">
        <br>

        <label>Email</label>
        <input name="email" id="emailRegister">
        <br>

        <label>Password</label>
        <input type="password" name="password" id="passwordRegister">
        <br>

        <button type="button" onclick="register()" id="buttonRegister">Register</button>
    </form>
</div>

<script src="/js/register.js"></script>